{
  "name": "vite-vue-vuex-boilerplate",
  "version": "0.1.0",
  "description": "Boilerplate of vite.js/vue.js/vuex/vue-router/TypeScript",
  "author": "okamoai <okamoai@rano-raraku.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/okamoai/vite-vue-vuex-boilerplate.git"
  },
  "engines": {
    "node": "~12.14.1",
    "npm": "~6.13.4"
  },
  "browserslist": [
    "ie 11",
    "last 1 Edge versions",
    "last 1 Firefox versions",
    "last 1 Chrome versions",
    "last 1 Safari versions",
    "last 2 iOS versions",
    "Android >= 6",
    "last 1 ChromeAndroid versions"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "prettier-eslint --write",
      "eslint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "vue": "^3.0.0-rc.7",
    "vue-router": "^4.0.0-beta.7",
    "vuex": "^4.0.0-beta.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "@vue/compiler-sfc": "^3.0.0-rc.7",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.0.0-beta.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "prettier-eslint-cli": "^5.0.0",
    "typescript": "^3.9.7",
    "vite": "^1.0.0-rc.4"
  }
}
